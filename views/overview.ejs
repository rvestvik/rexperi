<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" 
  "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<head>
<meta http-equiv="content-type" content="text/html; charset=utf-8">
<title>Rexperi</title>
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>
<script>
$(document).ready(function() {
  $("#clear_measurements").click(function() {
    $("#status").text("Cleared measurements!");
  });
});

</script>
</head>
<body>
<a id="clear_measurements">Clear measurements</a>
<div id="status"></div>
<div id="raw_measurements">
  <% measurements.forEach(function(measurement){ %>
    <div><%= measurement.timeStamp %>;<%= measurement.projectId %>;<%= measurement.eventId %>;<%= measurement.userId %><div>
  <% }) %>
</div>
</body>
</html>


